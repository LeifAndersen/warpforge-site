{% from "footer-macro.njk" import footerLink %}
{% set navPages = collections.all | eleventyNavigation %}
<footer>
    <a class="logo" href="/">
        <img width="235" height="51" src="/svg/logo.svg"/>
    </a>
    <div class="footer-navigation">
        {%- for entry in navPages %}
            {{ footerLink(entry) }}
        {%- endfor %}
    </div>
</footer>